</div>
<div class="unit-unit">
    @Html.EditorFor(model => model.ShowInMenus)
</div>


</div>
<div class="edit-unit">
<div class="unit-label">
    @Html.LabelFor(model => model.AllowComments):
</div>
<div class="unit-unit">
    @Html.EditorFor(model => model.AllowComments)
</div>


</div>
<div class="page-edit-top">
