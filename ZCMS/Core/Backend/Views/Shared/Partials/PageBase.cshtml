@model ZCMS.Core.Business.ZCMSPage
<div class="custom-properties-edit-wrap">
<div class="edit-unit">
<div class="edit-top-label">
    @Html.Label(CMS_i18n.BackendResources.PageID):
-
    @Html.Label(Model.PageID.ToString())
</div>
</div>
<div class="edit-unit">
<div class="unit-label">
